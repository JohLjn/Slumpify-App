{
  "expo": {
    "name": "Slumpify",
    "slug": "Slumpify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/slumpify-logo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-slumpify.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "splashScreenWaitMs": 2000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/slumpify-logo.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
